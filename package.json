  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-env": "^1.1.4",
    "babel-preset-stage-3": "^6.17.0",
    "cross-env": "^3.1.3",
